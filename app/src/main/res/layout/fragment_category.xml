            android:layout_marginVertical="20dp"
            android:layout_gravity="end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_22_cal" />
    </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_4" >


        <TextView
            android:id="@+id/textView_5"
            style="@style/textviewMarginStart"
            android:paddingRight="12dp"
            android:layout_gravity="start"
            android:layout_marginVertical="20dp"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:text="@string/name_of_item" />

        <TextView
            android:id="@+id/textView_cal5"
            style="@style/textviewMarginEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="20dp"
            android:layout_gravity="end"
            android:text="@string/_22_cal" />
    </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/card_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_5" >


        <TextView
            android:id="@+id/textView_6"
            style="@style/textviewMarginStart"
            android:paddingRight="12dp"
            android:layout_gravity="start"
            android:layout_marginVertical="20dp"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:text="@string/name_of_item" />

        <TextView
            android:id="@+id/textView_cal6"
            style="@style/textviewMarginEnd"
            android:layout_marginVertical="20dp"
            android:layout_gravity="end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_22_cal"/>
    </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_6" >


        <TextView
            android:id="@+id/textView_7"
            style="@style/textviewMarginStart"
            android:paddingRight="12dp"
