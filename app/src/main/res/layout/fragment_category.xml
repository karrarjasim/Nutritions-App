<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:scrollbars="none" >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_constraint_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:id="@+id/catogory"
            style="@style/titelOfCatogry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            android:text="@string/category_content"
            android:textColor="@color/black"
            android:layout_marginTop="18dp"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/card_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="50dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:cardPreventCornerOverlap="true"
            app:layout_constraintEnd_toEndOf="@+id/catogory"
            app:layout_constraintStart_toStartOf="@+id/catogory"
            app:layout_constraintTop_toBottomOf="@+id/catogory" >

            <TextView
                android:id="@+id/textView_1"
                style="@style/textviewMarginStart"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_marginVertical="20dp"
                android:layout_gravity="start"
                android:text="@string/name_of_item" />

            <TextView
                android:id="@+id/textView_cal1"
                style="@style/textviewMarginEnd"
                android:layout_marginVertical="20dp"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="@string/_22_cal"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_2"
            app:cardElevation="4dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_1" >


        <TextView
            android:id="@+id/textView_2"
            style="@style/textviewMarginStart"
            android:paddingRight="12dp"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginVertical="20dp"
            android:text="@string/name_of_item" />

        <TextView
            android:id="@+id/textView_cal2"
            style="@style/textviewMarginEnd"
            android:layout_marginVertical="20dp"
            android:layout_gravity="end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_22_cal" />
    </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:cardElevation="4dp"
            app:layout_constraintHorizontal_weight="2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_2">


            <TextView
                android:id="@+id/textView_3"
                style="@style/textviewMarginStart"
                android:layout_width="220dp"
                android:maxLength="12"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginVertical="20dp"
                android:paddingRight="12dp"
                android:text="@string/name_of_item" />

            <TextView
                android:id="@+id/textView_cal3"
                style="@style/textviewMarginEnd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginVertical="20dp"
                android:text="@string/_22_cal" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp"
            android:layout_marginHorizontal="24dp"
            android:layout_marginVertical="20dp"
            app:cardBackgroundColor="@color/textView_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_3" >


        <TextView
            android:id="@+id/textView_4"
            style="@style/textviewMarginStart"
            android:paddingRight="12dp"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginVertical="20dp"
            android:text="@string/name_of_item" />

        <TextView
            android:id="@+id/textView_cal4"
